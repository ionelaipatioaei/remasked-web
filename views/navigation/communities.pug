extends ../layout/layout.pug

include ../layout/header.pug
include ../components/community.pug

block header
  +Header(logged)

block title
  title Remasked | Your communities

block content
  h1 Your communities
  div(style="display: grid; grid-template-columns: repeat(5, 20%);")
    if communities
      each community in communities
        +Community(community)
    else
      h1 No communities